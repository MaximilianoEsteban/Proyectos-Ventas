@model FrontEnd.Carrefour.Models.LineaModel

<div>
    <div class="mb-5">
        <h1>Agregar Linea</h1>
    </div>
    <div>
        <form action="/Linea/Agregar" method="post" novalidate="novalidate">

            <div class="row">
                <div class="form-group col-md-3">
                    <label for="Descripcion">Descripcion de Linea</label>
                    <input type="text" class="form-control" id="Descripcion" name="Descripcion" />
                </div>
            </div>

            <div class="row">
                <div class="col-md-2">
                    <button type="submit" class="btn btn-primary ">Guardar</button>
                </div>
            </div>

        </form>
    </div>

</div>
@section Scripts {
    <script src="~/js/Linea/Agregar.js" asp-append-version="true"></script>
}




