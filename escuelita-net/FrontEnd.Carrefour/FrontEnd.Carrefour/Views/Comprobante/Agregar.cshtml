@model FrontEnd.Carrefour.Models.ComprobanteModel

<div>
    <div class="mb-5">
        <h1>Agregar Tipos de comprobante</h1>
    </div>
    <div>
        <form action="/Comprobante/Agregar" method="post" novalidate="novalidate">

            <div class="row">
                <div class="form-group col-md-3">
                    <label for="Comprobante">Tipos de Comprobantes</label>
                    <input type="text" class="form-control" id="Comprobante" name="Comprobante" />
                </div>
            </div>

            <div class="row">
                <div class="col-md-2">
                    <button type="submit" class="btn btn-primary ">Guardar</button>
                </div>
            </div>

        </form>
    </div>

</div>
@section Scripts {
    <script src="~/js/Comprobante/Agregar.js" asp-append-version="true"></script>
}




